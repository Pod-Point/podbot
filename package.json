{
  "name": "podbot",
  "version": "1.0.0",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run tsc && node lib/index.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "watch": "concurrently --kill-others \"tsc -w\" \"nodemon lib/index.js\"",
    "typings": "typings",
    "postinstall": "typings install",
    "test": "mocha --opts ./mocha.opts",
    "test:coverage": "tsc && node ./node_modules/istanbul/lib/cli.js cover _mocha lib/**/*.spec.js && ./node_modules/.bin/remap-istanbul -i ./coverage/coverage.raw.json -o ./coverage/html-report -t html",
    "test:coveralls": "npm run test:coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "aws-sdk": "^2.4.4",
    "botkit": "^0.2.2",
    "botkit-storage-redis": "^1.0.1",
    "concurrently": "^2.2.0",
    "config": "^1.21.0",
    "cron": "^1.1.0",
    "dotenv": "^2.0.0",
    "github": "^2.5.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^3.0.2",
    "nodemon": "^1.9.2",
    "proxyquire": "^1.7.10",
    "remap-istanbul": "^0.6.4",
    "sinon": "^1.17.5",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "tslint-microsoft-contrib": "^2.0.10"
  }
}
