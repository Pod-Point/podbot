{
  "name": "podbot",
  "version": "1.0.0",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run tsc && node lib/index.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "watch": "concurrently --kill-others \"tsc -w\" \"nodemon lib/index.js\"",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "dependencies": {
    "aws-sdk": "^2.4.4",
    "botkit": "^0.2.2",
    "botkit-storage-redis": "^1.0.1",
    "concurrently": "^2.2.0",
    "config": "^1.21.0",
    "cron": "^1.1.0",
    "dotenv": "^2.0.0",
    "github": "^2.5.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.3"
  },
  "devDependencies": {
    "nodemon": "^1.9.2",
    "tslint": "^3.15.1",
    "tslint-microsoft-contrib": "^2.0.10"
  }
}
