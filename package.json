{
  "name": "podbot",
  "version": "1.0.0",
  "main": "lib/index.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pod-Point/podbot.git"
  },
  "scripts": {
    "start": "npm run tsc && node lib/index.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "watch": "concurrently --kill-others \"tsc -w\" \"nodemon lib/index.js\"",
    "test": "mocha --opts ./mocha.opts",
    "test:coverage": "tsc && node ./node_modules/istanbul/lib/cli.js cover _mocha lib/**/*.spec.js && ./node_modules/.bin/remap-istanbul -i ./coverage/coverage.raw.json -o ./coverage/html-report -t html",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@types/chai": "^3.4.34",
    "@types/config": "0.0.30",
    "@types/cron": "^1.0.30",
    "@types/dotenv": "^2.0.19",
    "@types/mocha": "^2.2.33",
    "@types/node": "^6.0.51",
    "@types/proxyquire": "^1.3.27",
    "@types/sinon": "^1.16.32",
    "async": "^2.1.5",
    "aws-sdk": "^2.67.0",
    "botkit": "^0.5.8",
    "botkit-storage-redis": "^1.0.1",
    "concurrently": "^2.2.0",
    "config": "^1.21.0",
    "cron": "^1.1.0",
    "dotenv": "^2.0.0",
    "github": "^2.5.1",
    "node-trello": "^1.1.2",
    "typescript": "^2.5.2"
  },
  "devDependencies": {
    "@pod-point/eslint-config-podpoint-base": "github:pod-point/eslint-config-podpoint-base",
    "aws-sdk-mock": "^1.6.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-typescript": "^0.7.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^3.0.2",
    "nodemon": "^1.9.2",
    "proxyquire": "^1.7.10",
    "remap-istanbul": "^0.6.4",
    "sinon": "^1.17.5",
    "ts-node": "^1.3.0",
    "typescript-eslint-parser": "^8.0.0"
  }
}
